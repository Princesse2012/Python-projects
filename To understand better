#######################
word = ""
counter = 0
letters = ["c", "a", "r"]
while counter < len(letters):
  word = word + letters[counter]
  counter = counter + 1
print(word)
#############
result = 0
numbers = [2, 4, 6]
for num in numbers:
  result = result + num
print(result)
