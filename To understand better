################################
stuff = [
    ["aa", "bbb", "c", "d"],
    ["eeeee", "ff"],
]
 
biggest = ""
 
for i in stuff:
    for j in i:
        biggest = biggest if len(biggest) > len(j) else j
 
print(biggest)
